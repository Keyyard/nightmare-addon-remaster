{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "description": {
            "identifier": "minecraft:player",
            "is_spawnable": false,
            "is_summonable": false,
            "is_experimental": false,
            "animations": {
                "demon": "controller.animation.demon",
                "bird": "controller.animation.bird",
                "witch": "controller.animation.witch",
                "ring": "controller.animation.ring",
                "system": "controller.animation.system"
            },
            "scripts": {
                "animate": [
                    "demon",
                    "bird",
                    "witch",
                    "ring",
                    "system"
                ]
            }
        },
        "component_groups": {
            "demon": {
                "minecraft:variant": {
                    "value": 1
                }
            },
            "bird": {
                "minecraft:variant": {
                    "value": 2
                }
            },
            "witch": {
                "minecraft:variant": {
                    "value": 3
                }
            },
            "ring": {
                "minecraft:variant": {
                    "value": 4
                }
            },
            "keyyard:seeker": {
                "minecraft:nameable": {
                    "always_show": true
                },
                "minecraft:mark_variant": {
                    "value": 1
                }
            },
            "keyyard:hider": {
                "minecraft:nameable": {
                    "always_show": false
                },
                "minecraft:mark_variant": {
                    "value": 0
                }
            },
            "default": {
                "minecraft:variant": {
                    "value": 0
                }
            },
            "minecraft:add_bad_omen": {
                "minecraft:spell_effects": {
                    "add_effects": [{
                        "effect": "bad_omen",
                        "duration": 6000,
                        "display_on_screen_animation": true
                    }]
                },
                "minecraft:timer": {
                    "time": [
                        0.0,
                        0.0
                    ],
                    "looping": false,
                    "time_down_event": {
                        "event": "minecraft:clear_add_bad_omen",
                        "target": "self"
                    }
                }
            },
            "minecraft:clear_bad_omen_spell_effect": {
                "minecraft:spell_effects": {}
            },
            "minecraft:raid_trigger": {
                "minecraft:raid_trigger": {
                    "triggered_event": {
                        "event": "minecraft:remove_raid_trigger",
                        "target": "self"
                    }
                },
                "minecraft:spell_effects": {
                    "remove_effects": "bad_omen"
                }
            },
            "mount:pointer": {
                "minecraft:addrider": {
                    "entity_type": "entity:pointer"
                }
            }
        },
        "components": {
            "minecraft:experience_reward": {
                "on_death": "Math.Min(query.player_level * 7, 100)"
            },
            "minecraft:nameable": {
                "always_show": false
            },
            "minecraft:type_family": {
                "family": [
                    "player"
                ]
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:loot": {
                "table": "loot_tables/empty.json"
            },
            "minecraft:collision_box": {
                "width": 0.6,
                "height": 1.8
            },
            "minecraft:can_climb": {},
            "minecraft:movement": {
                "value": 0.1
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [{
                    "filters": {
                        "test": "in_lava",
                        "subject": "self",
                        "operator": "==",
                        "value": true
                    },
                    "cause": "lava",
                    "damage_per_tick": 4
                }]
            },
            "minecraft:attack": {
                "damage": 1
            },
            "minecraft:health": {
                "value": 2,
                "max": 2,
                "min": 2
            },
            "minecraft:player.saturation": {
                "value": 20
            },
            "minecraft:player.exhaustion": {
                "value": 0,
                "max": 4
            },
            "minecraft:player.level": {
                "value": 0,
                "max": 24791
            },
            "minecraft:player.experience": {
                "value": 0,
                "max": 1
            },
            "minecraft:breathable": {
                "total_supply": 15,
                "suffocate_time": -1,
                "inhale_time": 3.75,
                "generates_bubbles": false
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": false,
                "is_pushable_by_piston": true
            },
            "minecraft:insomnia": {
                "days_until_insomnia": 3
            },
            "minecraft:rideable": {
                "seat_count": 1,
                "family_types": [
                    "pointer"
                ],
                "seats": [{
                    "position": [
                        0, -0.9,
                        2.0
                    ],
                    "min_rider_count": 1,
                    "max_rider_count": 1
                }]
            },
            "minecraft:knockback_resistance": {
                "value": 1
            },
            "minecraft:conditional_bandwidth_optimization": {},
            "minecraft:environment_sensor": {
                "triggers": [{
                        "filters": {
                            "test": "has_tag",
                            "value": "seeker",
                            "subject": "self"
                        },
                        "event": "become:seeker"
                    },
                    {
                        "filters": {
                            "test": "has_tag",
                            "operator": "!=",
                            "subject": "self",
                            "value": "seeker"
                        },
                        "event": "become:hider"
                    },
                    {
                        "filters": {
                            "all_of": {
                                "test": "has_equipment",
                                "subject": "self",
                                "domain": "head",
                                "value": "item:demon"
                            }
                        },
                        "event": "demon"
                    },
                    {
                        "filters": {
                            "all_of": {
                                "test": "has_equipment",
                                "subject": "self",
                                "domain": "head",
                                "value": "item:bird"
                            }
                        },
                        "event": "bird"
                    },
                    {
                        "filters": {
                            "all_of": {
                                "test": "has_equipment",
                                "subject": "self",
                                "domain": "head",
                                "value": "item:witch"
                            }
                        },
                        "event": "witch"
                    },
                    {
                        "filters": {
                            "all_of": {
                                "test": "has_equipment",
                                "subject": "self",
                                "domain": "head",
                                "value": "item:ring"
                            }
                        },
                        "event": "ring"
                    },
                    {
                        "filters": {
                            "all_of": [{
                                    "test": "has_equipment",
                                    "subject": "self",
                                    "domain": "head",
                                    "operator": "not",
                                    "value": "item:demon"
                                },
                                {
                                    "test": "has_equipment",
                                    "subject": "self",
                                    "domain": "head",
                                    "operator": "not",
                                    "value": "item:witch"
                                },
                                {
                                    "test": "has_equipment",
                                    "subject": "self",
                                    "domain": "head",
                                    "operator": "not",
                                    "value": "item:bird"
                                },
                                {
                                    "test": "has_equipment",
                                    "subject": "self",
                                    "domain": "head",
                                    "operator": "not",
                                    "value": "item:ring"
                                }
                            ]
                        },
                        "event": "reset"
                    }
                ]
            },
            "minecraft:damage_sensor": {
                "triggers": [{
                    "on_damage": {
                        "filters": {
                            "all_of": [{
                                    "test": "has_tag",
                                    "subject": "other",
                                    "value": "seeker"
                                },
                                {
                                    "test": "has_tag",
                                    "operator": "!=",
                                    "subject": "self",
                                    "value": "seeker"
                                }
                            ]
                        },
                        "event": "become:seeker"
                    }
                }]
            }
        },
        "events": {
            "minecraft:entity_born": {
                "add": {
                    "component_groups": [
                        "keyyard:hider"
                    ]
                }
            },
            "become:seeker": {
                "add": {
                    "component_groups": [
                        "keyyard:seeker"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "keyyard:seeker"
                    ]
                }
            },
            "keyyard:role_reset": {
                "remove": {
                    "component_groups": [
                        "keyyard:seeker",
                        "keyyard:hider"
                    ]
                }
            },
            "become:hider": {
                "remove": {
                    "component_groups": [
                        "keyyard:seeker"
                    ]
                },
                "add": {
                    "component_groups": [
                        "keyyard:hider"
                    ]
                }
            },
            "mount:pointer": {
                "add": {
                    "component_groups": [
                        "mount:pointer"
                    ]
                }
            },
            "demon": {
                "add": {
                    "component_groups": [
                        "demon"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "default",
                        "bird",
                        "witch"
                    ]
                }
            },
            "ring": {
                "add": {
                    "component_groups": [
                        "ring"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "default",
                        "bird",
                        "witch",
                        "demon"
                    ]
                }
            },
            "bird": {
                "add": {
                    "component_groups": [
                        "bird"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "default",
                        "demon",
                        "witch"
                    ]
                }
            },
            "witch": {
                "add": {
                    "component_groups": [
                        "witch"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "default",
                        "demon",
                        "bird"
                    ]
                }
            },
            "reset": {
                "remove": {
                    "component_groups": [
                        "demon",
                        "bird",
                        "witch"
                    ]
                },
                "add": {
                    "component_groups": [
                        "default"
                    ]
                }
            },
            "minecraft:gain_bad_omen": {
                "add": {
                    "component_groups": [
                        "minecraft:add_bad_omen"
                    ]
                }
            },
            "minecraft:clear_add_bad_omen": {
                "remove": {
                    "component_groups": [
                        "minecraft:add_bad_omen"
                    ]
                },
                "add": {
                    "component_groups": [
                        "minecraft:clear_bad_omen_spell_effect"
                    ]
                }
            },
            "minecraft:trigger_raid": {
                "add": {
                    "component_groups": [
                        "minecraft:raid_trigger"
                    ]
                }
            },
            "minecraft:remove_raid_trigger": {
                "remove": {
                    "component_groups": [
                        "minecraft:raid_trigger"
                    ]
                }
            }
        }
    }
}