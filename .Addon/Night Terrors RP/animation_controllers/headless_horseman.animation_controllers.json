{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.entity.headless_horseman.move": {
            "initial_state": "default",
            "states": {
                "default": {
                    "transitions": [{
                        "move": "q.modified_move_speed > 0.05"
                    }]
                },
                "move": {
                    "animations": ["move"],
                    "transitions": [{
                        "default": "q.modified_move_speed <= 0.05"
                    }]
                }
            }
        }
    }
}